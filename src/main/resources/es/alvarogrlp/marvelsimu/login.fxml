<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="477.0" prefWidth="436.0" spacing="20.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.ies.puerto.controller.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <ImageView fitHeight="141.0" fitWidth="329.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../../../../../../../../../Descargas/logo.png" />
      </image>
   </ImageView>
    <Text fx:id="textUsuario" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Usuario" />
    <TextField fx:id="textFieldUsuario" prefHeight="24.0" prefWidth="197.0" />
    <Text fx:id="textContrasenia" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Password" />
    <PasswordField fx:id="textFieldPassword" />
    <Button fx:id="botonAceptar" alignment="TOP_CENTER" onAction="#onLoginButtonClick" prefHeight="26.0" prefWidth="163.0" text="Aceptar" textAlignment="CENTER" />
</VBox>
